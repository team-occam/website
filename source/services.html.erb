footer_heading: Lorem ipsum dolor amet



---

<header class="c-top-section c-top-section--short">
  <div class="o-container">
    <div class="o-grid">
      <h1 class="o-grid__item u-1/1 c-top-section__heading" data-aos="fade-up" data-aos-delay="100">Assume nothing, simplify everything</h1>
      <div class="o-grid__item u-1/1 u-1/3@md" data-aos="fade-up" data-aos-delay="150">
        <p class="label">Mission</p>
        <h4 class="c-top-section__subheading">To empower our partners through <br class="u-hide--medium-down">the simplicity of design.</h4>
      </div>
      <div class="o-grid__item u-1/1 u-1/3@md" data-aos="fade-up" data-aos-delay="200">
        <p class="label">Services</p>
        <ul class="c-top-section__list o-list o-list--inline">
          <% current_page.data.services.each do |services| %>
            <li class="c-top-section__list-item o-inline-list__item"><a href="<%services.url %>" class="c-link" target="_blank"><%= services.service_title %></a></li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
</header>

<section class="c-section">
  <div class="o-container o-container--narrow">
    
  </div>
</section>

<% content_for :page_scripts do %>
  <%= javascript_include_tag 'home' %>
<% end %>
